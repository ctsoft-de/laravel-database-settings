{
    "name": "ctsoft/laravel-database-settings",
    "description": "Database settings for Laravel with tenancy support.",
    "keywords": [
        "laravel",
        "settings",
        "database",
        "tenancy"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "CTSoft",
            "email": "info@ctsoft.de"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/database": "^6.0|^7.0|^8.0",
        "illuminate/encryption": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "CTSoft\\Laravel\\DatabaseSettings\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "CTSoft\\Laravel\\DatabaseSettings\\Providers\\DatabaseSettingsProvider"
            ],
            "aliases": {
                "Settings": "CTSoft\\Laravel\\DatabaseSettings\\Facades\\Settings"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
